                <item guid="afd5a499-b66a-40e7-8868-a57f1f3c462b" action="eval" useFieldsInCommand="false">
                    <command>
                        <body>set scrubbedList &quot;&quot;</body>
                    </command>
                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                </item>
                <item guid="7945e4e4-0892-4797-8b2b-c2e534c8423d" action="foreach" useFieldsInCommand="false">
                    <command>
                        <body>id $userIds name $usernames</body>
                    </command>
                    <nestedSteps>
                        <item guid="7181142e-c9ef-412d-be3b-17b0eba175f1" action="eval" useFieldsInCommand="false">
                            <command>
                                <body>lsearch -exact $ignoredUsers $name</body>
                            </command>
                            <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                        </item>
                        <item guid="71429d87-d52d-45af-b495-306580956267" action="if" useFieldsInCommand="false">
                            <command>
                                <body>[lsearch -exact $ignoredUsers $name] == -1</body>
                            </command>
                            <nestedSteps>
                                <item guid="271f3851-e6bf-4b3c-a74a-456cad1dff51" action="then" useFieldsInCommand="false">
                                    <nestedSteps>
                                        <item guid="e29579e7-0d82-4edd-889c-82ea06be7e54" action="eval" useFieldsInCommand="false">
                                            <command>
                                                <body>lappend scrubbedList &quot;userId = $id username = $name\\n&quot;</body>
                                            </command>
                                            <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                                        </item>
                                    </nestedSteps>
                                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                                </item>
                            </nestedSteps>
                            <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                        </item>
                    </nestedSteps>
                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                </item>
                <item guid="ef38a396-4b4e-4739-9662-d959c223de26" action="if" useFieldsInCommand="false">
                    <command>
                        <body>[llength $scrubbedList] == 0</body>
                    </command>
                    <nestedSteps>
                        <item guid="0adce538-0eae-4410-848c-085bd72ea5f0" action="then" useFieldsInCommand="false">
                            <nestedSteps>
                                <item guid="8a270bbe-dbd0-4ddb-aa5e-72ebe7c5fa6e" action="return">
                                    <command>
                                        <body>userId = none username = none</body>
                                    </command>
                                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                                </item>
                            </nestedSteps>
                            <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                        </item>
                        <item guid="ad727f8d-0f9f-48b8-bc51-99ea3f4d09ea" action="else" useFieldsInCommand="false">
                            <nestedSteps>
                                <item guid="0f4fd0f6-e988-4d4e-97ae-173ecd5ba397" action="return">
                                    <command>
                                        <body>$scrubbedList</body>
                                    </command>
                                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                                </item>
                            </nestedSteps>
                            <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                        </item>
                    </nestedSteps>
                    <applicationProperties type="com.fnfr.svt.documents.EmptyPropertyGroup"/>
                </item>
            </steps>
            <arguments>
                <item name="ignoredUsers">
                    <description>a space-separated list of usernames to ignore</description>
                    <defaultValue>_pdsvcacct</defaultValue>
                    <isMandatory>true</isMandatory>
                </item>
            </arguments>
            <response>{&quot;userInfo&quot;:{&quot;username&quot;:&quot;value&quot;,&quot;userId&quot;:&quot;value&quot;}}</response>
        </item>
